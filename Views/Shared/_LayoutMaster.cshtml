                }
                else if (x4 > 9999999999) {
                    alert("Add Valid Number ");
                    return false;
                }
                else if (x5 == "") {
                    alert("Alternative Contact No must be filled out");
                    return false;
                }
                else if (x5 > 9999999999) {
                    alert("Add Valid Number ");
                    return false;
                }
                else if (x6 == "") {
                    alert("Email must be filled out");
                    return false;
                }
                else if (x7 == "") {
                    alert("Password must be filled out");
                    return false;
                }
                else if (x8 == "") {
                    alert("Re Enter Password must be filled out");
                    return false;
                }
                else if (x7 != x8) {
                    alert("Enter Same password  must be filled out");
                    return false;
                }
                else if (x9 == "") {
                    alert("Date Of Birth must be filled out");
                    return false;
                }
                else if (x10 == "") {
                    alert("Role be filled out");
                    return false;
                }
                else if (x11 == "") {
                    alert("Gender must be filled out");
                    return false;
                }
                else {
                    var a = $("#fn").val();
                    var b = $("#ln").val();
                    var u = $("#un").val();
                    var o = $("#ocu").val();
                    var c = $("#n12").val();
                    var d = $("#nalt").val();
                    var e = $("#m1").val();
                    var f = $("#pass1").val();
                    var g = $("#d1").val();
                    var i = $("#g1").val();
