                        dataType: 'json',
                        success: function (data) {
                            console.log(data);


                            var str = "";

                          
                            
                            str += " <tr>";
                            str += "<th>Medicine Name</th>";
                            str += "  <th>Type</th>"
                            str += "  <th>Price</th>";
                            str += "  <th>Quantity</th>";
                            str += " <th>Total</th>";
                            str += " </tr>";

                            str += "<tr class='item-row'>";
                            str += "  <td class='item-name'><div class='delete-wpr'><textarea style='width:100px'>Medicine</textarea><a class='delete' href='javascript:;' title='Remove row'>X</a></div></td>";
                            str += " <td class='description' ><textarea style='width:100px'>injection/serup/tablet</textarea></td>";
                            str += "<td><textarea class='cost' style='width:80px'>$0.00</textarea></td>";
                            str += " <td><textarea class='qty' style='width:60px'>1</textarea></td>";
                            str += " <td><span class='price' style='width:30px'>$0.00</span></td>";
                            str += "</tr>";


                            str += "<tr>";
                            str += " <td colspan='5'><a id='addrow' href='javascript:;' title='Add a row'>Add a row</a></td>";
                            str += "</tr>";

                            str += "<tr>";
                            str += "<td colspan='2' class='blank'> </td>";
                            str += "<td colspan='2' class='total-line'>Subtotal</td>";
                            str += "<td class='total-value'><div id='subtotal'>$875.00</div></td>";
                            str += "</tr>";
                            str += "<tr>";
                            str += "<td colspan='2' class='blank'> </td>";
                            str += "<td colspan='2' class='total-line'>Total</td>";
                            str += "<td class='total-value'><div id='total'>$875.00</div></td>";
                            str += "</tr>";
                            str += "<tr>";
                            str += "<td colspan='2' class='blank'> </td>";
                            str += "<td colspan='2' class='total-line'>Amount Paid</td>";
                            str += " <td class='total-value'><textarea id='paid' style='height:30px;'>$0.00</textarea></td>";
                            str += "</tr>";
                            str += "<tr>";
                            str += " <td colspan='2' class='blank'> </td>";
                            str += " <td colspan='2' class='total-line balance'>Balance Due</td>";
                            str += " <td class='total-value balance'><div class='due'>$875.00</div></td>";
                            str += "</tr>";


                            $('#items').html(str);
                        },

                        error: function (msg) { alert(msg); }


                    });


                }

            </script>
