        <script>
            $(document).ready(function () {

                $.ajax({
                    type: 'GET',
                    url: "http://localhost:52257/api/medicines",
                    dataType: 'json',
                    success: function (data) {
                        console.log(data);

                        var str = " ";
                        for (var i = 0; i < data.length; i++) {
                            str += "<tr>";

                            str += "<td id='r1" + i + "'>" + data[i].MedicineName + "</td>";
                            str += "<td id='r2" + i + "'>" + data[i].MedicineWeight + "</td>";
                            str += "<td id='r3" + i + "'>" + data[i].MedicineCompany + "</td>";
                            str += "<td id='r4" + i + "'>" + data[i].Quantity + "</td>";
                            str += "<td id='r5" + i + "'>" + data[i].Price + "</td>";
                            str += "<td id='r6" + i + "'>" + data[i].Type + "</td>";
                            str += "<td>";
                            str += "<input type='button' onclick='add()' class='btn btn-block dropdown-toggle' value='Add' />";
                            str += "</td>";

                            str += "</tr>";
                        }
                        $('#med').html(str);
                    },

                    error: function (msg) { alert(msg); }
                });

            });

        </script>

