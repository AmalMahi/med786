<script>
    // code to read selected table row cell data (values).
    function abc() {


        $.ajax({
            type: 'GET',
            url: "http://med786.apphb.com/api/medicines",
            dataType: 'json',
            success: function (data) {
                console.log(data);
                if (data != null && data != "") {

                    var str = " ";
                    for (var i = 0; i < data.length; i++) {
                        str += "<tr>";
                        str += "<td id='r" + i + "'>" + data[i].MedicineName + "</td>";
                        str += "<td id='r" + i + "'>" + data[i].MedicineWeight + "</td>";
                        str += "<td id='r" + i + "'>" + data[i].MedicineCompany + "</td>";
                        str += "<td id='r" + i + "'>" + data[i].Price + "</td>";
                        str += "<td id='r" + i + "'>" + data[i].Type + "</td>";
                        str += "<td><a id='zaidi' onclick='xyz()' >Add</a></td>";
                        str += "</tr>";
                    }
                    $('#med').html(str);

                    $("#first").hide();

                    $("#second").show();

                    table = document.getElementById("qaz");
                    var rows = table.rows;

                    for (var i = 1; i < rows.length; i++) {

                        rows[i].onclick = (function (e) {
                            var rowid = (this.cells[0].innerHTML);
                            document.getElementById("MedName").setAttribute('value', rowid);
                            var rowweight = (this.cells[1].innerHTML);
                            document.getElementById("Medweight").setAttribute('value', rowweight);
                            var rowprice = (this.cells[3].innerHTML);
                            document.getElementById("Price").setAttribute('value', rowprice);
                            var rowtype = (this.cells[4].innerHTML);
                            document.getElementById("Type").setAttribute('value', rowtype);
                            var a = $("#o").val();
                            document.getElementById("Location").setAttribute('value', a);
                            var b = $("#p").val();
                            document.getElementById("MedStore").setAttribute('value', b);
                            var y = $("#date").val();
                            document.getElementById("date").setAttribute('value', y);
                            var data = localStorage.getItem("name");
                            document.getElementById("UserName").setAttribute('value', data);
                            var data2 = localStorage.getItem("name");
                            document.getElementById("Account2").setAttribute('value', data2);

                        });
                    }

                }
                else {
                    alert("Error");
                }
            },

            error: function (msg) { alert(msg); }

        });


        // get the current row



    }

</script>
